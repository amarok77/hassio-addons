{
  "name": "Dozzle",
  "version": "6.2.4",
  "slug": "dozzle",
  "description": "Container Log Viewer",
  "url": "https://github.com/amarok77/hassio-addons",
  "arch": [
    "amd64",
    "armhf",
    "aarch64",
    "i386"
  ],
  "startup": "application",
  "apparmor": false,
  "boot": "auto",
  "ports": {
    "8080/tcp": 8080
  },
  "ports_description": {
    "8080/tcp": "Web Admin"
  },
  "map": [
    "config:rw"
  ],
  "image": "amir20/dozzle:latest",
  "services": {
    "volume": "/var/run/docker.sock:/var/run/docker.sock"
  }, 
  "options": {},
  "schema": {},
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "webui": "http://[HOST]:[PORT:8080]",
  "init": false
}
