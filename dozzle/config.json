{
    "name": "Dozzle",
    "version": "v6.2.4",
    "slug": "dozzle",
    "image": "amir20/dozzle",
    "description": "BETA version of Dozzle Container Log viewer HA addon. Image based on upstream docker images.",
    "url": "https://github.com/amarok77/hassio-addons/tree/master/dozzle-beta",
    "startup": "system",    
    "map": ["ssl"],
    "boot": "auto",
    "ports": {"8080/tcp": 8080},
    "arch": ["armhf", "amd64", "armv7", "aarch64"],
    "options": {"env_var": []},
    "schema": {"env_var":[{"name": "str", "value": "str"}]},
    "webui": "http://[HOST]:[PORT:8080]"
}